</span><span><span class="hljs-comment">/* ====== RENDER &amp; UI ====== */</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">renderTable</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
  </span><span><span class="hljs-keyword">const</span></span><span> tbody=</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'tbody'</span></span><span>);
  tbody.</span><span><span class="hljs-property">innerHTML</span></span><span>=</span><span><span class="hljs-string">""</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> rows = </span><span><span class="hljs-title class_">Array</span></span><span>.</span><span><span class="hljs-title function_">from</span></span><span>(standings.</span><span><span class="hljs-title function_">values</span></span><span>()).</span><span><span class="hljs-title function_">map</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">s</span></span></span><span>=&gt;{
    </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-variable constant_">GD</span></span><span> = s.</span><span><span class="hljs-property">GF</span></span><span> - s.</span><span><span class="hljs-property">GA</span></span><span>;
    </span><span><span class="hljs-keyword">return</span></span><span> {...s, </span><span><span class="hljs-variable constant_">GD</span></span><span>};
  });

  rows.</span><span><span class="hljs-title function_">sort</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a,b</span></span></span><span>)=&gt;{
    </span><span><span class="hljs-keyword">return</span></span><span> b.</span><span><span class="hljs-property">Pts</span></span><span> - a.</span><span><span class="hljs-property">Pts</span></span><span>
      || (b.</span><span><span class="hljs-property">GF</span></span><span>-b.</span><span><span class="hljs-property">GA</span></span><span>) - (a.</span><span><span class="hljs-property">GF</span></span><span>-a.</span><span><span class="hljs-property">GA</span></span><span>)
      || b.</span><span><span class="hljs-property">GF</span></span><span> - a.</span><span><span class="hljs-property">GF</span></span><span>
