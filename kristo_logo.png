    </span><span><span class="hljs-keyword">const</span></span><span> row = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">'div'</span></span><span>);
