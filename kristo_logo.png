      &lt;td&gt;</span><span><span class="hljs-subst">${r.L}</span></span><span>&lt;/td&gt;
      &lt;td&gt;</span><span><span class="hljs-subst">${r.GF}</span></span><span>&lt;/td&gt;
      &lt;td&gt;</span><span><span class="hljs-subst">${r.GA}</span></span><span>&lt;/td&gt;
