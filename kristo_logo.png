      || (b.</span><span><span class="hljs-property">GF</span></span><span>-b.</span><span><span class="hljs-property">GA</span></span><span>) - (a.</span><span><span class="hljs-property">GF</span></span><span>-a.</span><span><span class="hljs-property">GA</span></span><span>)  </span><span><span class="hljs-comment">// GD</span></span><span>
