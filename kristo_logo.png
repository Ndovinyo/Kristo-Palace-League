  </span><span><span class="hljs-keyword">else</span></span><span> { h.</span><span><span class="hljs-property">D</span></span><span>=</span><span><span class="hljs-number">1</span></span><span>; a.</span><span><span class="hljs-property">D</span></span><span>=</span><span><span class="hljs-number">1</span></span><span>; h.</span><span><span class="hljs-property">Pts</span></span><span>=</span><span><span class="hljs-number">1</span></span><span>; a.</span><span><span class="hljs-property">Pts</span></span><span>=</span><span><span class="hljs-number">1</span></span><span>; }
