}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">addGameToMdUI</span></span><span>(</span><span><span class="hljs-params">md, g, indexInMd</span></span><span>){
  </span><span><span class="hljs-keyword">const</span></span><span> glist = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelector</span></span><span>(</span><span><span class="hljs-string">`[data-glist="<span class="hljs-subst">${md}</span></span></span><span>"]`);
