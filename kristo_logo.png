    </span><span><span class="hljs-string">"Applejuicee"</span></span><span>,
    </span><span><span class="hljs-string">"Kristo Palace"</span></span><span>,
    </span><span><span class="hljs-string">"Sam"</span></span><span>,
    </span><span><span class="hljs-string">"kellyiscool"</span></span><span>,
    </span><span><span class="hljs-string">"Owen"</span></span><span>,
    </span><span><span class="hljs-string">"Geoffery Mosiria"</span></span><span>,
    </span><span><span class="hljs-string">"Vj Patelo (wakudumu)"</span></span><span>,
    </span><span><span class="hljs-string">"massive fc"</span></span><span>,
    </span><span><span class="hljs-string">"Callisto"</span></span><span>,
    </span><span><span class="hljs-string">"Tatsuya Moussa"</span></span><span>,
    </span><span><span class="hljs-string">"Young De Bruyne"</span></span><span>,
    </span><span><span class="hljs-string">"Dee"</span></span><span>,
    </span><span><span class="hljs-string">"Maziwaaa"</span></span><span>,
    </span><span><span class="hljs-string">"Rustix"</span></span><span>
  ];

  </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">generateFixtures</span></span><span>(</span><span><span class="hljs-params">teams</span></span><span>) {
    </span><span><span class="hljs-keyword">if</span></span><span> (teams.</span><span><span class="hljs-property">length</span></span><span> % </span><span><span class="hljs-number">2</span></span><span> !== </span><span><span class="hljs-number">0</span></span><span>) {
      teams.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">"BYE"</span></span><span>); </span><span><span class="hljs-comment">// For odd teams</span></span><span>
    }

    </span><span><span class="hljs-keyword">const</span></span><span> numTeams = teams.</span><span><span class="hljs-property">length</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> numRounds = numTeams - </span><span><span class="hljs-number">1</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> half = numTeams / </span><span><span class="hljs-number">2</span></span><span>;

    </span><span><span class="hljs-keyword">const</span></span><span> rounds = [];

    </span><span><span class="hljs-keyword">const</span></span><span> teamList = [...teams];
    </span><span><span class="hljs-keyword">const</span></span><span> fixed = teamList.</span><span><span class="hljs-title function_">shift</span></span><span>();

    </span><span><span class="hljs-keyword">for</span></span><span> (</span><span><span class="hljs-keyword">let</span></span><span> round = </span><span><span class="hljs-number">0</span></span><span>; round &lt; numRounds; round++) {
      </span><span><span class="hljs-keyword">const</span></span><span> matches = [];
      </span><span><span class="hljs-keyword">const</span></span><span> firstHalf = [fixed, ...teamList.</span><span><span class="hljs-title function_">slice</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>, half - </span><span><span class="hljs-number">1</span></span><span>)];
      </span><span><span class="hljs-keyword">const</span></span><span> secondHalf = [...teamList.</span><span><span class="hljs-title function_">slice</span></span><span>(half - </span><span><span class="hljs-number">1</span></span><span>)].</span><span><span class="hljs-title function_">reverse</span></span><span>();

      </span><span><span class="hljs-keyword">for</span></span><span> (</span><span><span class="hljs-keyword">let</span></span><span> i = </span><span><span class="hljs-number">0</span></span><span>; i &lt; firstHalf.</span><span><span class="hljs-property">length</span></span><span>; i++) {
        </span><span><span class="hljs-keyword">if</span></span><span> (firstHalf[i] !== </span><span><span class="hljs-string">"BYE"</span></span><span> &amp;&amp; secondHalf[i] !== </span><span><span class="hljs-string">"BYE"</span></span><span>) {
          matches.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">`<span class="hljs-subst">${firstHalf[i]}</span></span></span><span> vs </span><span><span class="hljs-subst">${secondHalf[i]}</span></span><span>`);
        }
      }

      rounds.</span><span><span class="hljs-title function_">push</span></span><span>(matches);

      </span><span><span class="hljs-comment">// Rotate teams</span></span><span>
      teamList.</span><span><span class="hljs-title function_">splice</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, teamList.</span><span><span class="hljs-title function_">pop</span></span><span>());
    }

    </span><span><span class="hljs-keyword">return</span></span><span> rounds;
  }

  </span><span><span class="hljs-keyword">const</span></span><span> fixtures = </span><span><span class="hljs-title function_">generateFixtures</span></span><span>(teams);

  </span><span><span class="hljs-keyword">const</span></span><span> container = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"fixtures"</span></span><span>);

  fixtures.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">round, index</span></span></span><span>) =&gt; {
    </span><span><span class="hljs-keyword">const</span></span><span> roundDiv = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"div"</span></span><span>);
    roundDiv.</span><span><span class="hljs-property">className</span></span><span> = </span><span><span class="hljs-string">"matchday"</span></span><span>;
    roundDiv.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">`&lt;h2&gt;Matchday <span class="hljs-subst">${index + <span class="hljs-number">1</span></span></span></span><span>}&lt;/h2&gt;`;
    round.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">match</span></span></span><span> =&gt; {
      </span><span><span class="hljs-keyword">const</span></span><span> matchDiv = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"div"</span></span><span>);
      matchDiv.</span><span><span class="hljs-property">className</span></span><span> = </span><span><span class="hljs-string">"fixture"</span></span><span>;
      matchDiv.</span><span><span class="hljs-property">textContent</span></span><span> = match;
      roundDiv.</span><span><span class="hljs-title function_">appendChild</span></span><span>(matchDiv);
