      &lt;span class="vsep"&gt;vs&lt;/span&gt;
      &lt;input type="number" min="0" value="</span><span><span class="hljs-subst">${<span class="hljs-keyword">as</span></span></span><span>}" aria-label="</span><span><span class="hljs-subst">${m.away}</span></span><span> score" /&gt;
