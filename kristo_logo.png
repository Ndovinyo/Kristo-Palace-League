      </span><span><span class="hljs-keyword">if</span></span><span> (arr.</span><span><span class="hljs-property">length</span></span><span> === </span><span><span class="hljs-number">0</span></span><span>) resultsByMatchday.</span><span><span class="hljs-title function_">delete</span></span><span>(m);
