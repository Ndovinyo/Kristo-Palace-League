  </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-keyword">if</span></span><span> (hs &lt; </span><span><span class="hljs-keyword">as</span></span><span>){ A.</span><span><span class="hljs-property">W</span></span><span>++; H.</span><span><span class="hljs-property">L</span></span><span>++; A.</span><span><span class="hljs-property">Pts</span></span><span> += </span><span><span class="hljs-number">3</span></span><span>; }
