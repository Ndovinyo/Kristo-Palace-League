  arr.</span><span><span class="hljs-title function_">push</span></span><span>({home, away, hs, </span><span><span class="hljs-keyword">as</span></span><span>});
  resultsByMatchday.</span><span><span class="hljs-title function_">set</span></span><span>(md, arr);

