  </span><span><span class="hljs-keyword">const</span></span><span> hs = </span><span><span class="hljs-built_in">parseInt</span></span><span>(</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'homeScore'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>, </span><span><span class="hljs-number">10</span></span><span>);
