    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">path</span></span></span><span> </span><span><span class="hljs-attr">d</span></span><span>=</span><span><span class="hljs-string">"M35 120 Q30 90 50 80 Q60 75 60 65 Q55 55 50 50 Q45 45 50 40 Q55 35 65 40 Q75 45 75 55 Q78 70 68 80 Q62 90 70 100 Q75 110 65 120 Q55 130 50 130 Q40 130 35 120 Z"</span></span><span> </span><span><span class="hljs-attr">fill</span></span><span>=</span><span><span class="hljs-string">"url(#goldGradient)"</span></span><span> </span><span><span class="hljs-attr">stroke</span></span><span>=</span><span><span class="hljs-string">"#333"</span></span><span> </span><span><span class="hljs-attr">stroke-width</span></span><span>=</span><span><span class="hljs-string">"2"</span></span><span>/&gt;
    </span><span><span class="hljs-comment">&lt;!-- Right Lion (mirrored) --&gt;</span></span><span>
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">g</span></span></span><span> </span><span><span class="hljs-attr">transform</span></span><span>=</span><span><span class="hljs-string">"scale(-1,1) translate(-200,0)"</span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">path</span></span></span><span> </span><span><span class="hljs-attr">d</span></span><span>=</span><span><span class="hljs-string">"M35 120 Q30 90 50 80 Q60 75 60 65 Q55 55 50 50 Q45 45 50 40 Q55 35 65 40 Q75 45 75 55 Q78 70 68 80 Q62 90 70 100 Q75 110 65 120 Q55 130 50 130 Q40 130 35 120 Z"</span></span><span> </span><span><span class="hljs-attr">fill</span></span><span>=</span><span><span class="hljs-string">"url(#goldGradient)"</span></span><span> </span><span><span class="hljs-attr">stroke</span></span><span>=</span><span><span class="hljs-string">"#333"</span></span><span> </span><span><span class="hljs-attr">stroke-width</span></span><span>=</span><span><span class="hljs-string">"2"</span></span><span>/&gt;
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">g</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">svg</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;
