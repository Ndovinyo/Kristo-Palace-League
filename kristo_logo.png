</span><span><span class="hljs-comment">/* Round-robin (circle method), then randomize ordering */</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">generateRoundRobin</span></span><span>(</span><span><span class="hljs-params">teams</span></span><span>){
  </span><span><span class="hljs-keyword">const</span></span><span> n = teams.</span><span><span class="hljs-property">length</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> list = </span><span><span class="hljs-title function_">shuffle</span></span><span>([...teams]); </span><span><span class="hljs-comment">// randomize starting order</span></span><span>
  </span><span><span class="hljs-keyword">const</span></span><span> rounds = [];
  </span><span><span class="hljs-keyword">const</span></span><span> half = n/</span><span><span class="hljs-number">2</span></span><span>;

  </span><span><span class="hljs-keyword">const</span></span><span> rotating = list.</span><span><span class="hljs-title function_">slice</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>); </span><span><span class="hljs-comment">// first is fixed</span></span><span>
  </span><span><span class="hljs-keyword">for</span></span><span>(</span><span><span class="hljs-keyword">let</span></span><span> r=</span><span><span class="hljs-number">0</span></span><span>; r&lt;n-</span><span><span class="hljs-number">1</span></span><span>; r++){
    </span><span><span class="hljs-keyword">const</span></span><span> left = [list[</span><span><span class="hljs-number">0</span></span><span>], ...rotating.</span><span><span class="hljs-title function_">slice</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>, half-</span><span><span class="hljs-number">1</span></span><span>)];
    </span><span><span class="hljs-keyword">const</span></span><span> right = rotating.</span><span><span class="hljs-title function_">slice</span></span><span>(half-</span><span><span class="hljs-number">1</span></span><span>).</span><span><span class="hljs-title function_">reverse</span></span><span>();
    </span><span><span class="hljs-keyword">const</span></span><span> matches=[];
    </span><span><span class="hljs-keyword">for</span></span><span>(</span><span><span class="hljs-keyword">let</span></span><span> i=</span><span><span class="hljs-number">0</span></span><span>;i&lt;left.</span><span><span class="hljs-property">length</span></span><span>;i++){
      </span><span><span class="hljs-keyword">const</span></span><span> home = left[i];
      </span><span><span class="hljs-keyword">const</span></span><span> away = right[i];
      matches.</span><span><span class="hljs-title function_">push</span></span><span>({home, away, </span><span><span class="hljs-attr">hs</span></span><span>:</span><span><span class="hljs-literal">null</span></span><span>, </span><span><span class="hljs-attr">as</span></span><span>:</span><span><span class="hljs-literal">null</span></span><span>, </span><span><span class="hljs-attr">applied</span></span><span>:</span><span><span class="hljs-literal">false</span></span><span>});
    }
    rounds.</span><span><span class="hljs-title function_">push</span></span><span>(matches);
    </span><span><span class="hljs-comment">// rotate</span></span><span>
    rotating.</span><span><span class="hljs-title function_">unshift</span></span><span>(rotating.</span><span><span class="hljs-title function_">pop</span></span><span>());
  }

  </span><span><span class="hljs-comment">// Randomize round order and match order within each round</span></span><span>
  </span><span><span class="hljs-title function_">shuffle</span></span><span>(rounds).</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">r</span></span></span><span> =&gt; </span><span><span class="hljs-title function_">shuffle</span></span><span>(r));
  </span><span><span class="hljs-keyword">return</span></span><span> rounds;
}

