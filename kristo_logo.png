  </span><span><span class="hljs-keyword">if</span></span><span> (!</span><span><span class="hljs-title class_">Number</span></span><span>.</span><span><span class="hljs-built_in">isFinite</span></span><span>(hs) || !</span><span><span class="hljs-title class_">Number</span></span><span>.</span><span><span class="hljs-built_in">isFinite</span></span><span>(</span><span><span class="hljs-keyword">as</span></span><span>) || hs &lt; </span><span><span class="hljs-number">0</span></span><span> || </span><span><span class="hljs-keyword">as</span></span><span> &lt; </span><span><span class="hljs-number">0</span></span><span>){
