    </span><span><span class="hljs-keyword">const</span></span><span> m = </span><span><span class="hljs-built_in">parseInt</span></span><span>(mStr,</span><span><span class="hljs-number">10</span></span><span>), i = </span><span><span class="hljs-built_in">parseInt</span></span><span>(iStr,</span><span><span class="hljs-number">10</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> arr = resultsByMatchday.</span><span><span class="hljs-title function_">get</span></span><span>(m) || [];
    </span><span><span class="hljs-keyword">if</span></span><span> (i&gt;=</span><span><span class="hljs-number">0</span></span><span> &amp;&amp; i&lt;arr.</span><span><span class="hljs-property">length</span></span><span>){
      arr.</span><span><span class="hljs-title function_">splice</span></span><span>(i,</span><span><span class="hljs-number">1</span></span><span>);
